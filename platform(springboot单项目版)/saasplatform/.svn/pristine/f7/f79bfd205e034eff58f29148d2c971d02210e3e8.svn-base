<!DOCTYPE html>
<html>

<head>
<meta charset="utf-8">
<title></title>
<link rel="stylesheet" href="css/bootstrap.css" />
<script type="text/javascript" src="js/app.bdimg.com.jquery.js"></script>
<style type="text/javascript" src="js/bootstrap.js"></style>
<style>
.progress-bar {
	border-radius: 50%;
}
</style>
</head>
<script src="//code.jquery.com/jquery-1.12.4.min.js"></script>
<script type="text/javascript"
	src="http://malsup.github.io/jquery.form.js"></script>
<body>
	<form id="formId" method="post">
		<h1>ajax使用spring mvc提供的类的方法上传文件</h1>
		<input type="file" name="img"> 
		<select name="type">
			<option value="1" >123</option>
			<option value="2" >2</option>
		</select>
		<input type="file" name="head"> 
		<input type="file" name="file"> 
		<input type="submit" onclick="upload()" value="提交"/>
	</form>
	<button onclick="logout()" value="退出登录">退出登录</button>
<button onclick="test()" value="获得类型">创建月</button>
<button onclick="testAjax()" value="获得类型">测试AJAX异常</button>
<button onclick="testAjaxCustom()" value="获得类型">测试AJAX自定义异常</button>
<br/>
</body>
<script type="text/javascript">
function testAjaxCustom() {
	$.ajax({
		url : '/custom/exception',
		loadtip : true,
		type : "POST",
		dataType : 'json',
		success : function(resp) {
			if (resp.code == 0) {
				i++;
				createPayRecordMonth();
			} else {
				alert(resp.msg);
			}
		}
	});
}
function testAjax() {
	$.ajax({
		url : '/exception',
		loadtip : true,
		type : "POST",
		dataType : 'json',
		success : function(resp) {
			if (resp.code == 0) {
				i++;
				createPayRecordMonth();
			} else {
				alert(resp.msg);
			}
		}
	});
}
function test() {
	$.ajax({
		url : '/hyplatform/test',
		loadtip : true,
		type : "POST",
		dataType : 'json',
		data:{
			'limit':date,
			'offset':date
		},
		success : function(resp) {
			if (resp.code == 0) {
				i++;
				createPayRecordMonth();
			} else {
				alert(resp.msg);
			}
		}
	});
}
	function logout() {
		$.ajax({
			url : '/SpringMvc/logout',
			loadtip : true,
			type : "POST",
			dataType : 'json',
			success : function(resp) {
				debugger;
				if (resp.code == 0) {
					alert(resp.msg);
				} else {
					alert(resp.msg);
				}
			}
		});
	}
</script>
</html>